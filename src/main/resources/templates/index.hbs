{{#partial "content"}}

    <h1>Team list!</h1>

    {{#if teams}} <!--if teams exist, then-->
    <p>Team Signups:</p>
    {{#each teams }} <!--loop-->

    <p>{{ teamName }} <a href="/teams/{{id}}">read more...</a></p> <!--show-->
    <p>{{ createdAt }}</p>
    {{/each}}
    {{else}} <!--no teams!-->
    <p>No teams!</p>
    {{/if}}


{{/partial}}